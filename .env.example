# DateDriven - API Keys Configuration
# Copy this file to .env and add your actual API keys

# Anthropic Claude API (https://console.anthropic.com/)
CLAUDE_API_KEY=your_anthropic_key_here

# OpenAI GPT API (https://platform.openai.com/)
OPENAI_API_KEY=your_openai_key_here

# Google Gemini API (https://ai.google.dev/)
GEMINI_API_KEY=your_gemini_key_here

# xAI Grok API (https://x.ai/)
GROK_API_KEY=your_xai_key_here

# Google Calendar (optional - for calendar sync)
GOOGLE_CALENDAR_ID=primary

# eBay API (optional - for automatic pricing)
# Get credentials at https://developer.ebay.com/
EBAY_CLIENT_ID=your_ebay_client_id
EBAY_CLIENT_SECRET=your_ebay_client_secret
EBAY_REFRESH_TOKEN=your_ebay_refresh_token

# 3DSellers API (optional alternative to eBay direct)
THREE_D_SELLERS_API_KEY=your_3dsellers_key
